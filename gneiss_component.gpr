
with "gneiss";

abstract project Gneiss_Component is

   Gneiss_Root := external ("GNEISS_ROOT", ".");
   Lib_Dir := "libcomponents";

   Library_Prefix := "component_";

   for Library_Options use ("-L" & Gneiss_Root & "/ada-runtime/obj/adalib",
                            "-nostdlib",
                            "-nostdinc",
                            "-lgnat");

end Gneiss_Component;
